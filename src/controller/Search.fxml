<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>

<AnchorPane xmlns="http://javafx.com/javafx/9" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.SearchController">
   <children>
      <Pane prefHeight="550.0" prefWidth="500.0">
         <children>
            <TextField fx:id="edtSearch" layoutX="13.0" layoutY="40.0" onKeyPressed="#handleKeyPressed" onKeyReleased="#completeSearch" prefHeight="26.0" prefWidth="475.0" />
            <ListView fx:id="lstSearch" layoutX="14.0" layoutY="72.0" prefHeight="432.0" prefWidth="475.0" />
            <Button fx:id="btnCancel" layoutX="394.0" layoutY="512.0" mnemonicParsing="false" onMousePressed="#close" prefHeight="26.0" prefWidth="92.0" text="Cancelar" />
            <Button fx:id="btnOK" layoutX="293.0" layoutY="512.0" mnemonicParsing="false" prefHeight="26.0" prefWidth="92.0" text="OK" />
            <Label alignment="CENTER" layoutX="14.0" layoutY="7.0" prefHeight="26.0" prefWidth="475.0" text="Label" />
         </children>
      </Pane>
   </children>
</AnchorPane>
